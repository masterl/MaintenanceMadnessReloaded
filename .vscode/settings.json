{
    "editor.rulers": [
        80
    ],
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.suggest.showWords": false,
    "editor.formatOnSave": true,
    "Lua.runtime.version": "Lua 5.4",
    "Lua.workspace.preloadFileSize": 1600,
    "Lua.diagnostics.globals": [
        "game",
        "script",
        "remote",
        "commands",
        "settings",
        "rcon",
        "rendering",
        "global",
        "log",
        "defines",
        "data",
        "mods",
        "serpent",
        "table_size",
        "bit32",
        "util",
        "localised_print",
        "circuit_connector_definitions",
        "default_circuit_wire_max_distance",
        "universal_connector_template",
        "__DebugAdapter",
        "__Profiler",
        "insulate",
        "describe",
        "it",
        "setup",
        "default_orange_color",
        "default_dirt"
    ],
    "Lua.workspace.ignoreDir": [
        ".vscode",
        ".github",
        ".script",
        "locale"
    ],
    "Lua.runtime.builtin": {
        "basic": "enable",
        "builtin": "enable",
        "table": "enable",
        "math": "enable",
        "string": "enable",
        "package": "enable",
        "debug": "enable",
        "bit32": "enable",
        "io": "disable",
        "jit": "disable",
        "bit": "disable",
        "coroutine": "disable",
        "ffi": "disable",
        "os": "disable",
        "utf8": "disable"
    },
    "Lua.runtime.plugin": ".vscode/lua/plugin.lua",
    "Lua.workspace.library": [
        "script",
        "rendering",
        "game",
        "commands"
    ],
    "Lua.diagnostics.workspaceDelay": 50,
    "git.autofetch": "all",
    "markdownlint.config": {
        "MD033": false,
        "MD034": false,
    },
}
